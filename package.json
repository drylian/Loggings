{
  "name": "loggings",
  "version": "2.0b",
  "description": "Simple log system for your node js application, supported colors.",
  "scripts": {
    "build": "tsx tools/build.ts",
    "deno:dev": "deno src/deno.ts",
    "test": "tsx --watch tests/index.ts",
    "build:dev": "tsx --watch tools/build.ts",
    "npm:update": "tsc tools/build.ts && npm run build && npm run test && npm version minor && npm publish"
  },
  "type": "module",
  "main": "dist/index.min.cjs",
  "module": "dist/index.min.mjs",
  "types": "dist/types/node.d.ts",
  "files": [
    "dist",
    "cdn.html"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drylian/Loggings.git"
  },
  "keywords": [
    "loggings",
    "logs",
    "logger",
    "log",
    "register",
    "colors",
    "log",
    "npm",
    "node"
  ],
  "author": "Drylian",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/drylian/Loggings/issues"
  },
  "homepage": "https://github.com/drylian/Loggings#readme",
  "devDependencies": {
    "@types/node": "^20.11.27",
    "esbuild": "^0.21.0",
    "esbuild-dts-path-alias": "^4.2.1",
    "tsx": "^4.17.0"
  }
}
